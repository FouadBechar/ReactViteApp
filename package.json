{
  "name": "react-vite-app",
  "version": "0.1.0",
  "private": true,
  "author": "Fouad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FouadBechar/ReactViteApp.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "generate-sitemap": "node scripts/generate-sitemap.mjs",
    "prebuild": "npm run generate-sitemap",
    "build": "vite build",
    "preview": "vite preview",
