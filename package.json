    "lint:css": "stylelint \"src/**/*.{css,scss,less}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sitemap": "^8.0.0",
